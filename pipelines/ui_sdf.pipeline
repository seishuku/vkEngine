descriptorSet {
	addBinding(0, combinedSampler, fragment)
}

pipeline {
	addStage(base64"", vertex)
	addStage(base64"", fragment)

	pushConstant(0, 80, vertex|fragment)

	subpass(0)
	topology(triangleStrip)
	cullMode(back)
	rasterizationSamples(1)

	blend(true)
	srcColorBlendFactor(srcAlpha)
	dstColorBlendFactor(oneMinusSrcAlpha)
	colorBlendOp(add)
	srcAlphaBlendFactor(srcAlpha)
	dstAlphaBlendFactor(oneMinusSrcAlpha)
	alphaBlendOp(add)

	addVertexBinding(0, 16, perVertex)
	addVertexAttribute(0, 0, rgba32_sfloat, 0)

	addVertexBinding(1, 48, perInstance)
	addVertexAttribute(1, 1, rgba32_sfloat, 0)
	addVertexAttribute(2, 1, rgba32_sfloat, 16)
	addVertexAttribute(3, 1, rgba32_uint, 32)
}
